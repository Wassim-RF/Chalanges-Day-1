#include <stdio.h>
#include <math.h>

typedef struct point1 {
    int x;
    int y;
    int z;
} point1;
typedef struct point2 {
    int x;
    int y;
    int z;
} point2;


int main() {
    point1 point1;
    point2 point2;
    int X , Y , Z ;
    int racine;

    printf("Entrer les coordonne du premiere point : \n");
    printf("  X : ");
    scanf("%d" , &point1.x);
    printf("  Y : ");
    scanf("%d" , &point1.y);
    printf("  Z : ");
    scanf("%d" , &point1.z);
    printf("Entrer les coordonne du premiere point : \n");
    printf("  X : ");
    scanf("%d" , &point2.x);
    printf("  Y : ");
    scanf("%d" , &point2.y);
    printf("  Z : ");
    scanf("%d" , &point2.z);
    X = pow(2 , (point2.x - point1.x));
    Y = pow(2 , (point2.y - point1.y));
    Z = pow(2 , (point2.z - point1.z));
    racine = sqrt(X + Y + Z);
    printf("Votre distance entre le point 1 et le point 2 est : %d ." , racine);
    return 0;
}